msgstr ""

#. type: Plain text
#: raw/manpages-dev/man2/accept.2:44
msgid "accept, accept4 - accept a connection on a socket"
msgstr ""

#. type: SH
#: raw/manpages-dev/man2/accept.2:44
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: raw/manpages-dev/man2/accept.2:48
#, no-wrap
msgid ""
"B<#include E<lt>sys/types.hE<gt>>          /* See NOTES */\n"
"B<#include E<lt>sys/socket.hE<gt>>\n"
msgstr ""

