#: raw/manpages-dev/man2/send.2:478
msgid ""
"B<fcntl>(2), B<getsockopt>(2), B<recv>(2), B<select>(2), B<sendfile>(2), "
"B<sendmmsg>(2), B<shutdown>(2), B<socket>(2), B<write>(2), B<cmsg>(3), "
"B<ip>(7), B<socket>(7), B<tcp>(7), B<udp>(7)"
msgstr ""

