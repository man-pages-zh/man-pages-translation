#
#. type: TH
#: raw/coreutils/man1/mkdir.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#
#. type: SH
#: raw/coreutils/man1/mkdir.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#
#. type: Plain text
#: raw/coreutils/man1/mkdir.1:5
msgid "mkdir - make directories"
msgstr "mkdir - 创建目录"

#
#. type: SH
#: raw/coreutils/man1/mkdir.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "概述"

#. type: Plain text
#: raw/coreutils/man1/mkdir.1:8
msgid "B<mkdir> [I<\\,OPTION\\/>]... I<\\,DIRECTORY\\/>..."
msgstr "B<mkdir> [I<\\,选项\\/>]... I<\\,目录列表\\/>..."

#
#. type: SH
#: raw/coreutils/man1/mkdir.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#
#. type: Plain text
#: raw/coreutils/man1/mkdir.1:12
msgid "Create the DIRECTORY(ies), if they do not already exist."
msgstr "如果目录列表中的目录不存在，则创建之。"

#
#. type: Plain text
#: raw/coreutils/man1/mkdir.1:14
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr "必选参数对长短选项同时适用。"

#. type: TP
#: raw/coreutils/man1/mkdir.1:14
#, no-wrap
msgid "B<-m>, B<--mode>=I<\\,MODE\\/>"
msgstr "B<-m>, B<--mode>=I<\\,模式\\/>"

#
#. type: Plain text
#: raw/coreutils/man1/mkdir.1:17
msgid "set file mode (as in chmod), not a=rwx - umask"
msgstr "设置文件模式(类似chmod)，而不是 rwx 减 umask"

#
#. type: TP
#: raw/coreutils/man1/mkdir.1:17
#, no-wrap
msgid "B<-p>, B<--parents>"
msgstr "B<-p>, B<--parents>"

#
#. type: Plain text
#: raw/coreutils/man1/mkdir.1:20
msgid "no error if existing, make parent directories as needed"
msgstr "如果目录已存在，不报错，且按需创建父目录"

#
#. type: TP
#: raw/coreutils/man1/mkdir.1:20
#, no-wrap
msgid "B<-v>, B<--verbose>"
msgstr "B<-v>, B<--verbose>"

#
#. type: Plain text
#: raw/coreutils/man1/mkdir.1:23
msgid "print a message for each created directory"
msgstr "为每一个已创建的目录打印信息"

#
#. type: TP
#: raw/coreutils/man1/mkdir.1:23
#, no-wrap
msgid "B<-Z>"
msgstr "B<-Z>"

#
#. type: Plain text
#: raw/coreutils/man1/mkdir.1:27
msgid ""
"set SELinux security context of each created directory to the default type"
msgstr "将每个创建的目录的 SELinux 安全上下文设置为默认类型"

#. type: TP
#: raw/coreutils/man1/mkdir.1:27
#, no-wrap
msgid "B<--context>[=I<\\,CTX\\/>]"
msgstr "B<--context>[=I<\\,CTX\\/>]"

#
#. type: Plain text
#: raw/coreutils/man1/mkdir.1:31
msgid ""
"like B<-Z>, or if CTX is specified then set the SELinux or SMACK security "
"context to CTX"
msgstr ""
"类似 B<-Z>，或者如果指定了上下文，则将 SELinux 或者 SMACK 安全上下文设置为指"
"定类型"

#
#. type: TP
#: raw/coreutils/man1/mkdir.1:31
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#
#. type: Plain text
#: raw/coreutils/man1/mkdir.1:34
msgid "display this help and exit"
msgstr "显示此帮助信息并退出"

#
#. type: TP
#: raw/coreutils/man1/mkdir.1:34
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#
#. type: Plain text
#: raw/coreutils/man1/mkdir.1:37
msgid "output version information and exit"
msgstr "显示版本信息并退出"

#
#. type: SH
#: raw/coreutils/man1/mkdir.1:37
#, no-wrap
msgid "AUTHOR"
msgstr "作者"

#
