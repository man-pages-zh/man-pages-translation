#
#. type: Plain text
#: raw/coreutils/man1/printenv.1:5
msgid "printenv - print all or part of environment"
msgstr "printenv - 显示全部或部分环境变量"

#
#. type: SH
#: raw/coreutils/man1/printenv.1:5
#, no-wrap
msgid "SYNOPSIS"
