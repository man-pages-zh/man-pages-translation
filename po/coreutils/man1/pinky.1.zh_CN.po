#
#. type: TH
#: raw/coreutils/man1/pinky.1:2
#, no-wrap
msgid "PINKY"
msgstr "PINKY"

#. type: TH
#: raw/coreutils/man1/pinky.1:2
#, no-wrap
msgid "January 2017"
msgstr "2017年1月"

#. type: TH
#: raw/coreutils/man1/pinky.1:2
#, no-wrap
msgid "GNU coreutils 8.26"
msgstr "GNU coreutils 8.26"

#
#. type: TH
#: raw/coreutils/man1/pinky.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#
#. type: SH
#: raw/coreutils/man1/pinky.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#
#. type: Plain text
#: raw/coreutils/man1/pinky.1:5
msgid "pinky - lightweight finger"
msgstr "pinky - 轻量级的 finger"

#
#. type: SH
#: raw/coreutils/man1/pinky.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "概述"

#
#. type: Plain text
#: raw/coreutils/man1/pinky.1:8
msgid "B<pinky> [I<\\,OPTION\\/>]... [I<\\,USER\\/>]..."
msgstr "B<pinky> [I<\\,选项\\/>]... [I<\\,用户\/>]..."

#
#. type: SH
#: raw/coreutils/man1/pinky.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#
#. type: TP
#: raw/coreutils/man1/pinky.1:10
#, no-wrap
msgid "B<-l>"
msgstr "B<-l>"

#
#. type: Plain text
#: raw/coreutils/man1/pinky.1:13
msgid "produce long format output for the specified USERs"
msgstr "对指定用户输出完整的长格式信息"

#
#. type: TP
#: raw/coreutils/man1/pinky.1:13
#, no-wrap
msgid "B<-b>"
msgstr "B<-b>"

#
#. type: Plain text
#: raw/coreutils/man1/pinky.1:16
msgid "omit the user's home directory and shell in long format"
msgstr "在长格式中省略用户主目录以及登录 shell"

#
#. type: TP
#: raw/coreutils/man1/pinky.1:16
#, no-wrap
msgid "B<-h>"
msgstr "B<-h>"

#
#. type: Plain text
#: raw/coreutils/man1/pinky.1:19
msgid "omit the user's project file in long format"
msgstr "在长格式中省略用户的项目文件"

#
#. type: TP
#: raw/coreutils/man1/pinky.1:19
#, no-wrap
msgid "B<-p>"
msgstr "B<-p>"

#
#. type: Plain text
#: raw/coreutils/man1/pinky.1:22
msgid "omit the user's plan file in long format"
msgstr "在长格式中省略用户的计划文件"

#
#. type: TP
#: raw/coreutils/man1/pinky.1:22
#, no-wrap
msgid "B<-s>"
msgstr "B<-s>"

#
#. type: Plain text
#: raw/coreutils/man1/pinky.1:25
msgid "do short format output, this is the default"
msgstr "输出短格式信息（默认）"

#
#. type: TP
#: raw/coreutils/man1/pinky.1:25
#, no-wrap
msgid "B<-f>"
msgstr "B<-f>"

#
#. type: Plain text
#: raw/coreutils/man1/pinky.1:28
msgid "omit the line of column headings in short format"
msgstr "在短格式中省略列栏报头"

#
#. type: TP
#: raw/coreutils/man1/pinky.1:28
#, no-wrap
msgid "B<-w>"
msgstr "B<-w>"

#
#. type: Plain text
#: raw/coreutils/man1/pinky.1:31
msgid "omit the user's full name in short format"
msgstr "在短格式中省略用户全名"

#
#. type: TP
#: raw/coreutils/man1/pinky.1:31
#, no-wrap
msgid "B<-i>"
msgstr "B<-i>"

#
#. type: Plain text
#: raw/coreutils/man1/pinky.1:34
msgid "omit the user's full name and remote host in short format"
msgstr "在短格式中省略用户全名以及远程主机"

#
#. type: TP
#: raw/coreutils/man1/pinky.1:34
#, no-wrap
msgid "B<-q>"
msgstr "B<-q>"

#
#. type: Plain text
#: raw/coreutils/man1/pinky.1:38
msgid "omit the user's full name, remote host and idle time in short format"
msgstr "在短格式中省略用户全名、远程主机以及空闲时间"

#
#. type: TP
#: raw/coreutils/man1/pinky.1:38
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#
#. type: Plain text
#: raw/coreutils/man1/pinky.1:41
msgid "display this help and exit"
msgstr "显示此帮助信息并退出"

#
#. type: TP
#: raw/coreutils/man1/pinky.1:41
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#
#. type: Plain text
#: raw/coreutils/man1/pinky.1:44
msgid "output version information and exit"
msgstr "显示版本信息并退出"

#
#. type: Plain text
#: raw/coreutils/man1/pinky.1:47
msgid ""
"A lightweight 'finger' program; print user information.  The utmp file will "
"be I<\\,/var/run/utmp\\/>."
msgstr "一个轻量级的“finger”程序；输出用户信息。所用 utmp 文件是 I<\\,/var/run/utmp\\/>。"

#
#. type: SH
#: raw/coreutils/man1/pinky.1:47
#, no-wrap
msgid "AUTHOR"
msgstr "作者"

#
#. type: Plain text
#: raw/coreutils/man1/pinky.1:49
msgid "Written by Joseph Arceneaux, David MacKenzie, and Kaveh Ghazi."
msgstr "由 Joseph Arceneaux、David MacKenzie 和 Kaveh Ghazi 编写。"

#
#. type: SH
#: raw/coreutils/man1/pinky.1:49
#, no-wrap
msgid "REPORTING BUGS"
msgstr "报告错误"

#
#. type: Plain text
#: raw/coreutils/man1/pinky.1:51
msgid ""
"GNU coreutils online help: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr "GNU coreutils 的在线帮助： E<lt>http://www.gnu.org/software/coreutils/E<gt>"

#
#. type: Plain text
#: raw/coreutils/man1/pinky.1:53
msgid ""
"Report pinky translation bugs to E<lt>http://translationproject.org/team/"
"E<gt>"
msgstr ""
"请向 E<lt>http://translationproject.org/team/zh_CN.htmlE<gt> 报告 pinky 的翻译错误。"

#
#. type: SH
#: raw/coreutils/man1/pinky.1:53
#, no-wrap
msgid "COPYRIGHT"
msgstr "版权"

