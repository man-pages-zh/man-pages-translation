#
#. type: Plain text
#: raw/coreutils/man1/dd.1:5
msgid "dd - convert and copy a file"
msgstr "dd - 转换并复制一个文件"

#
#. type: SH
#: raw/coreutils/man1/dd.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "概述"

#
#. type: Plain text
#: raw/coreutils/man1/dd.1:8
msgid "B<dd> [I<\\,OPERAND\\/>]..."
msgstr "B<dd> [I<\\,操作符\\/>]..."

#
#. type: Plain text
#: raw/coreutils/man1/dd.1:11
msgid "B<dd> I<\\,OPTION\\/>"
msgstr "B<dd> I<\\,选项\\/>"

#
#. type: SH
