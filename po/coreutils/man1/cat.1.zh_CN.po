#
#. type: TH
#: raw/coreutils/man1/cat.1:2
#, no-wrap
msgid "CAT"
msgstr "CAT"

#. type: TH
#: raw/coreutils/man1/cat.1:2
#, no-wrap
msgid "October 2017"
msgstr "2017年10月"

#. type: TH
#: raw/coreutils/man1/cat.1:2
#, no-wrap
msgid "GNU coreutils 8.28"
msgstr "GNU 核心工具 8.28"

#
#. type: TH
#: raw/coreutils/man1/cat.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#
#. type: SH
#: raw/coreutils/man1/cat.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:5
msgid "cat - concatenate files and print on the standard output"
msgstr "cat - 连接文件并在标准输出上输出"

#
#. type: SH
#: raw/coreutils/man1/cat.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "概述"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:8
msgid "B<cat> [I<\\,OPTION\\/>]... [I<\\,FILE\\/>]..."
msgstr "B<cat> [I<\\,选项\\/>]... [I<\\,文件\\/>]..."

#
#. type: SH
#: raw/coreutils/man1/cat.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:12
msgid "Concatenate FILE(s) to standard output."
msgstr "将文件列表中的文件连接到标准输出。"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:14
msgid "With no FILE, or when FILE is -, read standard input."
msgstr "如果没有指定文件，或者指定文件为“-”，则从标准输入读取。"

#
#. type: TP
#: raw/coreutils/man1/cat.1:14
#, no-wrap
msgid "B<-A>, B<--show-all>"
msgstr "B<-A>, B<--显示-所有>"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:17
msgid "equivalent to B<-vET>"
msgstr "等价于 B<-vET>"

#
#. type: TP
#: raw/coreutils/man1/cat.1:17
#, no-wrap
msgid "B<-b>, B<--number-nonblank>"
msgstr "B<-b>, B<--编号-非空>"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:20
msgid "number nonempty output lines, overrides B<-n>"
msgstr "给非空输出行编号，使 B<-n> 失效。"

#
#. type: TP
#: raw/coreutils/man1/cat.1:20
#, no-wrap
msgid "B<-e>"
msgstr "B<-e>"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:23
msgid "equivalent to B<-vE>"
msgstr "等价于 B<-vE>"

#
#. type: TP
#: raw/coreutils/man1/cat.1:23
#, no-wrap
msgid "B<-E>, B<--show-ends>"
msgstr "B<-E>, B<--显示-结束>"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:26
msgid "display $ at end of each line"
msgstr "在每行结束显示 $"

#
#. type: TP
#: raw/coreutils/man1/cat.1:26
#, no-wrap
msgid "B<-n>, B<--number>"
msgstr "B<-n>, B<--编号>"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:29
msgid "number all output lines"
msgstr "给所有输出行编号"

#
#. type: TP
#: raw/coreutils/man1/cat.1:29
#, no-wrap
msgid "B<-s>, B<--squeeze-blank>"
msgstr "B<-s>, B<--压缩-空格>"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:32
msgid "suppress repeated empty output lines"
msgstr "将所有的连续的多个空行替换为一个空行"

#
#. type: TP
#: raw/coreutils/man1/cat.1:32
#, no-wrap
msgid "B<-t>"
msgstr "B<-t>"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:35
msgid "equivalent to B<-vT>"
msgstr "等价于 B<-vT>"

#
#. type: TP
#: raw/coreutils/man1/cat.1:35
#, no-wrap
msgid "B<-T>, B<--show-tabs>"
msgstr "B<-T>, B<--显示标签>"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:38
msgid "display TAB characters as ^I"
msgstr "把 TAB 字符显示为 ^I"

#
#. type: TP
#: raw/coreutils/man1/cat.1:38
#, no-wrap
msgid "B<-u>"
msgstr "B<-u>"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:41
msgid "(ignored)"
msgstr "（被忽略的选项）"

#
#. type: TP
#: raw/coreutils/man1/cat.1:41
#, no-wrap
msgid "B<-v>, B<--show-nonprinting>"
msgstr "B<-v>, B<--显示，不打印>"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:44
msgid "use ^ and M- notation, except for LFD and TAB"
msgstr "除了 LFD 和 TAB 之外的不可打印字符，用 ^ 和 M- 标记方式显示"

#
#. type: TP
#: raw/coreutils/man1/cat.1:44
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:47
msgid "display this help and exit"
msgstr "显示此帮助信息并退出"

#
#. type: TP
#: raw/coreutils/man1/cat.1:47
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:50
msgid "output version information and exit"
msgstr "显示版本信息并退出"

#
#. type: SH
#: raw/coreutils/man1/cat.1:50
#, no-wrap
msgid "EXAMPLES"
msgstr "范例"

#
#. type: TP
#: raw/coreutils/man1/cat.1:51
#, no-wrap
msgid "cat f - g"
msgstr "cat f - g"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:54
msgid "Output f's contents, then standard input, then g's contents."
msgstr "依次输出文件 f，标准输入和文件 g 到标准输出"

#
#. type: TP
#: raw/coreutils/man1/cat.1:54
#, no-wrap
msgid "cat"
msgstr "cat"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:57
msgid "Copy standard input to standard output."
msgstr "复制标准输入到标准输出"

#
#. type: SH
#: raw/coreutils/man1/cat.1:57
#, no-wrap
msgid "AUTHOR"
msgstr "作者"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:59
msgid "Written by Torbjorn Granlund and Richard M. Stallman."
msgstr "由 Torbjorn Granlund 和 Richard M. Stallman 编写。"

#
#. type: SH
#: raw/coreutils/man1/cat.1:59
#, no-wrap
msgid "REPORTING BUGS"
msgstr "报告错误"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:61
msgid ""
"GNU coreutils online help: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr ""
"GNU coreutils 的在线帮助： E<lt>http://www.gnu.org/software/coreutils/E<gt>"

#
#. type: Plain text
#: raw/coreutils/man1/cat.1:63
msgid ""
"Report cat translation bugs to E<lt>http://translationproject.org/team/E<gt>"
msgstr ""
"报告 cat 的翻译错误请转到 E<lt>http://translationproject.org/team/zh_CN.htmlE<gt> 。"


