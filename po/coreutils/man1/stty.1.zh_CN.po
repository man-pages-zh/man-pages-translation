#
#. type: SH
#: raw/coreutils/man1/stty.1:14
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#
#. type: Plain text
#: raw/coreutils/man1/stty.1:18
msgid "Print or change terminal characteristics."
msgstr "打印或更改终端特性。"

#
#. type: Plain text
#: raw/coreutils/man1/stty.1:20
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr "必选参数对长短选项同时适用。"

#
#. type: TP
#: raw/coreutils/man1/stty.1:20
#, no-wrap
msgid "B<-a>, B<--all>"
msgstr "B<-a>, B<--all>"

#
#. type: Plain text
#: raw/coreutils/man1/stty.1:23
msgid "print all current settings in human-readable form"
msgstr "以人类可读的形式打印所有当前设置"

#
#. type: TP
#: raw/coreutils/man1/stty.1:23
#, no-wrap
msgid "B<-g>, B<--save>"
msgstr "B<-g>, B<--save>"

#
#. type: Plain text
#: raw/coreutils/man1/stty.1:26
msgid "print all current settings in a stty-readable form"
msgstr "以stty-readable形式打印所有当前设置"

#
#. type: TP
#: raw/coreutils/man1/stty.1:26
#, no-wrap
msgid "B<-F>, B<--file>=I<\\,DEVICE\\/>"
msgstr ""

#
#. type: Plain text
#: raw/coreutils/man1/stty.1:29
msgid "open and use the specified DEVICE instead of stdin"
msgstr "打开并使用指定的DEVICE而不是stdin"

#
#. type: TP
#: raw/coreutils/man1/stty.1:29
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#
#. type: Plain text
#: raw/coreutils/man1/stty.1:32
msgid "display this help and exit"
msgstr "显示此帮助信息并退出"

#
#. type: TP
#: raw/coreutils/man1/stty.1:32
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#
#. type: Plain text
#: raw/coreutils/man1/stty.1:35
msgid "output version information and exit"
msgstr "显示版本信息并退出"

#
#. type: Plain text
#: raw/coreutils/man1/stty.1:38
msgid ""
"Optional - before SETTING indicates negation.  An * marks non-POSIX "
"settings.  The underlying system defines which settings are available."
msgstr "可选 - 在SETTING指示否定之前。 An *标记非POSIX设置。 底层系统定义了可用的设置。"

#
#. type: SS
#: raw/coreutils/man1/stty.1:38
#, no-wrap
msgid "Special characters:"
msgstr "特殊字符："

#
#. type: TP
#: raw/coreutils/man1/stty.1:39
#, no-wrap
msgid "* discard CHAR"
msgstr "丢弃CHAR"

#
#. type: Plain text
#: raw/coreutils/man1/stty.1:42
