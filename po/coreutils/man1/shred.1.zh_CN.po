msgstr "B<-v>, B<--verbose>"

#. type: Plain text
#: raw/coreutils/man1/shred.1:23
msgid "overwrite N times instead of the default (3)"
msgstr ""

#. type: TP
#: raw/coreutils/man1/shred.1:23
#, fuzzy, no-wrap
msgid "B<--random-source>=I<\\,FILE\\/>"
msgstr "B<--random-source>=I<文件>"

#. type: Plain text
