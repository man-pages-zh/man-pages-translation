#. type: SH
#: raw/coreutils/man1/tac.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "概述"

#. type: Plain text
#: raw/coreutils/man1/tac.1:8
msgid "B<tac> [I<\\,OPTION\\/>]... [I<\\,FILE\\/>]..."
msgstr "B<tac> [I<\\,选项\\/>]... [I<\\,文件列表\\/>]..."

#. type: SH
#: raw/coreutils/man1/tac.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: raw/coreutils/man1/tac.1:12
msgid "Write each FILE to standard output, last line first."
msgstr "将文件列表中的每一个文件输出到标准输出，行号大的优先。"

#. type: Plain text
#: raw/coreutils/man1/tac.1:14
msgid "With no FILE, or when FILE is -, read standard input."
msgstr "如果没有指定文件，或者指定文件为 \"-\"，则从标准输入读取。"

#. type: Plain text
#: raw/coreutils/man1/tac.1:16
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr "必选参数对长短选项同时适用。"

#. type: TP
#: raw/coreutils/man1/tac.1:16
#, no-wrap
msgid "B<-b>, B<--before>"
msgstr "B<-b>, B<--before>"

#. type: Plain text
#: raw/coreutils/man1/tac.1:19
msgid "attach the separator before instead of after"
msgstr "将分隔符附加在行首而不是行尾"

#. type: TP
#: raw/coreutils/man1/tac.1:19
#, no-wrap
msgid "B<-r>, B<--regex>"
msgstr "B<-r>, B<--regex>"

#. type: Plain text
#: raw/coreutils/man1/tac.1:22
msgid "interpret the separator as a regular expression"
msgstr "将分隔符视为正则表达式"

#. type: TP
#: raw/coreutils/man1/tac.1:22
#, no-wrap
msgid "B<-s>, B<--separator>=I<\\,STRING\\/>"
msgstr "B<-s>, B<--separator>=I<\\,字符串\\/>"

#. type: Plain text
#: raw/coreutils/man1/tac.1:25
msgid "use STRING as the separator instead of newline"
msgstr "以指定字符串作为分隔符而不是换行符"

#. type: TP
#: raw/coreutils/man1/tac.1:25
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: raw/coreutils/man1/tac.1:28
msgid "display this help and exit"
msgstr "显示此帮助信息并退出"

#. type: TP
#: raw/coreutils/man1/tac.1:28
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: raw/coreutils/man1/tac.1:31
msgid "output version information and exit"
