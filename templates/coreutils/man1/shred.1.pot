#: raw/coreutils/man1/shred.1:46
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: raw/coreutils/man1/shred.1:49
msgid "display this help and exit"
msgstr ""

#. type: TP
#: raw/coreutils/man1/shred.1:49
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: raw/coreutils/man1/shred.1:52
